@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="number" name="score" />
    <button>Send</button>
</form>
<ul>
    @foreach (var item in Model)
    {
        <li>@item <a asp-controller="SortedSet" asp-action="Delete" asp-route-name="@item">Delete</a></li>
    }
</ul>